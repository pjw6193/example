package dayone;

public class Zoo {

	// JVM -> main -> run() -> method()
	
	public static void main(String[] args) {
		// reference variables, loop counters, etc. -- stack 
		Animal a = new Animal(); // heap  -- object memory
		Animal b = new Animal();
		a.version = "789";
		System.out.println(a.version);
		System.out.println(b.version);
	}
	
}


class Animal {
	static String version = "123456";
	// state - instance variables
	// wrapper class
	Integer weight = 2_000_000_000;
	boolean hasHair = true;
	char c = 'a';
	double d = 78.8889; // money - BigDecimal  (Long - Long) 
	float f = 78.8889f; // money - BigDecimal 
	long l = 1_000_000_000_000L;
	short s = 32_000;
	byte b = 127;
	
	
	String var = new String("string"); // object
	
	
	// behavior
	public void run() {
		System.out.println("Running");
	}
	
}



